{
  "name": "@naturalcycles/js-lib",
  "version": "5.0.1",
  "scripts": {
    "build": "del ./dist && tsc",
    "build-prod": "yarn build-prod-esm & yarn build-prod-cjs & wait",
    "build-prod-esm": "del ./dist-esm && tsc -p tsconfig.prod.json --outDir ./dist-esm --module es2015 --declaration false",
    "build-prod-cjs": "del ./dist && tsc -p tsconfig.prod.json"
  },
  "dependencies": {
    "@types/lru-cache": "^5.1.0",
    "@types/luxon": "^1.12.0",
    "lru-cache": "^5.1.1"
  },
  "devDependencies": {
    "@naturalcycles/dev-lib": "^7.0.0",
    "@naturalcycles/promise-lib": "^2.2.0",
    "@naturalcycles/semantic-release": "^1.0.0",
    "@types/benchmark": "^1.0.31",
    "@types/node": "^12.0.0",
    "benchmark": "^2.1.4",
    "jest": "^24.1.0",
    "type-fest": "^0.4.1"
  },
  "files": [
    "dist",
    "dist-esm",
    "src",
    "!src/test",
    "!src/**/*.test.*",
    "!src/**/__snapshots__"
  ],
  "main": "dist/index.js",
  "module": "dist-esm/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=8.11"
  },
  "description": "",
  "author": "Natural Cycles Team",
  "license": "MIT"
}
