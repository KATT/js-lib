{
  "name": "@naturalcycles/js-lib",
  "version": "8.11.0",
  "scripts": {
    "build": "del ./dist && tsc",
    "build-prod": "yarn build-prod-esm & yarn build-prod-cjs & wait",
    "build-prod-esm": "del ./dist-esm && tsc -p tsconfig.prod.json --outDir ./dist-esm --module es2015 --declaration false",
    "build-prod-cjs": "del ./dist && tsc -p tsconfig.prod.json"
  },
  "dependencies": {
    "type-fest": "^0.8.0"
  },
  "devDependencies": {
    "@naturalcycles/dev-lib": "^9.0.0",
    "@naturalcycles/test-lib": "^1.0.2",
    "@types/benchmark": "^1.0.31",
    "@types/node": "^12.0.0",
    "benchmark": "^2.1.4",
    "jest": "^24.1.0",
    "rxjs": "^6.5.2"
  },
  "files": [
    "dist",
    "dist-esm",
    "src",
    "!src/test",
    "!src/**/*.test.*",
    "!src/**/__snapshots__"
  ],
  "main": "dist/index.js",
  "module": "dist-esm/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=8.11"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NaturalCycles/js-lib"
  },
  "description": "Standard library for universal (browser + Node.js) javascript",
  "author": "Natural Cycles Team",
  "license": "MIT"
}
