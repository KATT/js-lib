{
  "name": "@naturalcycles/js-lib",
  "version": "4.5.1",
  "scripts": {
    "build": "del ./dist && tsc",
    "build-prod": "yarn build-prod-esm & yarn build-prod-cjs & wait",
    "build-prod-esm": "del ./dist-esm && tsc -p tsconfig.prod.json --outDir ./dist-esm --module es2015 --declaration false",
    "build-prod-cjs": "del ./dist && tsc -p tsconfig.prod.json"
  },
  "dependencies": {
    "@types/lru-cache": "^5.1.0",
    "@types/luxon": "^1.4.0"
  },
  "peerDependencies": {
    "lru-cache": ">=5.0.0"
  },
  "devDependencies": {
    "@naturalcycles/semantic-release": "^1.0.0",
    "@naturalcycles/shared-module": "^5.1.0",
    "@types/benchmark": "^1.0.31",
    "@types/jest": "^24.0.6",
    "@types/node": "^11.11.3",
    "benchmark": "^2.1.4",
    "jest": "^24.1.0",
    "jest-junit": "^6.2.1",
    "lru-cache": "^5.1.1",
    "luxon": "^1.4.3",
    "prettier": "^1.14.3",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2"
  },
  "files": [
    "dist",
    "dist-esm",
    "src",
    "!src/test",
    "!src/**/*.test.*",
    "!src/**/__snapshots__"
  ],
  "main": "dist/index.js",
  "module": "dist-esm/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=8.11"
  },
  "description": "",
