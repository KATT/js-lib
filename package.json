{
  "name": "@naturalcycles/js-lib",
  "scripts": {
    "build": "del ./dist && tsc",
    "build-prod": "yarn build-prod-esm & yarn build-prod-cjs & wait",
    "build-prod-esm": "del ./dist-esm && tsc -p tsconfig.prod.json --outDir ./dist-esm --module es2015 --declaration false",
    "build-prod-cjs": "del ./dist && tsc -p tsconfig.prod.json"
  },
  "dependencies": {
    "@types/lru-cache": "^4.1.1",
    "@types/luxon": "^1.4.0"
  },
  "peerDependencies": {
    "lru-cache": ">=4.0.0"
  },
  "devDependencies": {
    "@naturalcycles/shared-module": "^1.0.25",
    "@semantic-release/changelog": "^3.0.1",
    "@semantic-release/git": "^7.0.5",
    "@types/jest": "^23.3.5",
    "@types/node": "^10.0.0",
    "jest": "^23.6.0",
    "jest-junit": "^5.2.0",
    "lru-cache": "^4.1.3",
    "luxon": "^1.4.3",
    "prettier": "^1.14.3",
    "semantic-release": "^15.10.8",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.0"
  },
  "files": [
    "dist",
    "dist-esm",
    "src",
    "!src/scripts",
    "!src/test",
